{
  "swagger": "2.0",
  "info": {
    "title": "IgniteQuoting",
    "version": "1.0.0"
  },"host": "quoting-3scale-apicast-production.3scale.apps.ocp39pablo.rhtechofficelatam.com:443",
  "basePath": "/",
  "schemes": [
      "https"
  ],
  "paths": {
    "/webhook/QuoteAPI": {
      "post": {
        "summary": "Execute an Auto Insurance Quoting",
        "operationId": "quote",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "QuoteRequest",
            "schema": {
              "$ref": "#/definitions/QuoteRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Quote Response",
            "schema": {
              "$ref": "#/definitions/QuoteResponse"
            }
          }
        },
        "security": [
          {
            "apiKey": []
          },
          {
            "oauth2": []
          }
        ]
      }
    }
  },
  "definitions": {
    "QuoteRequest": {
      "properties": {
        "driver": {
          "$ref": "#/definitions/Driver"
        },
        "vehicle": {
          "$ref": "#/definitions/Vehicle"
        }
      }
    },
    "Driver": {
      "required": [
        "age",
        "driverID"
      ],
      "properties": {
        "driverID": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "fines": {
          "type": "integer"
        },
        "age": {
          "type": "integer"
        }
      }
    },
    "Vehicle": {
      "properties": {
        "licensePlate": {
          "type": "string"
        },
        "mileage": {
          "type": "integer"
        },
        "maker": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "year": {
          "type": "integer"
        }
      }
    },
    "QuoteResponse": {
      "properties": {
        "price": {
          "type": "integer"
        }
      }
    }
  },
  "securityDefinitions": {
    "apiKey": {
      "type": "apiKey",
      "name": "user-key",
      "in": "header"
    },
    "oauth2":{
    	"type": "oauth2",
    	"flow": "accessCode"
    	
    }
  },
  "security": [
    {
      "apiKey": []
    },
    {
    	"oauth2":[]
    }
  ]
}
